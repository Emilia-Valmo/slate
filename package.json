{
  "private": true,
  "name": "@gitbook/slate-packages",
  "workspaces": [
    "packages/*"
  ],
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "7.2.0",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@gitbook/slate-dev-benchmark": "*",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "chalk": "^1.1.3",
    "commander": "^2.15.1",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.9",
    "emojis": "^1.0.10",
    "emotion": "^9.2.4",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-prettier": "^2.5.0",
    "eslint-plugin-react": "^7.6.0",
    "faker": "^3.1.0",
    "figures": "^2.0.0",
    "flow-bin": "^0.93.0",
    "fs-promise": "^1.0.0",
    "gh-pages": "^0.11.0",
    "html-webpack-plugin": "^3.2.0",
    "html-webpack-template": "^6.2.0",
    "image-extensions": "^1.1.0",
    "immutable": ">=4.0.0-rc.12",
    "is-hotkey": "^0.1.1",
    "is-url": "^1.2.2",
    "jest": "^24.0.0-alpha.9",
    "jsdom": "^11.5.1",
    "lerna": "^2.7.1",
    "lodash": "^4.17.4",
    "mini-css-extract-plugin": "^0.5.0",
    "npm-run-all": "^4.1.2",
    "prettier": "^1.10.2",
    "prismjs": "^1.5.1",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-emotion": "^9.2.4",
    "react-hot-loader": "^4.6.3",
    "react-portal": "^4.1.5",
    "react-router-dom": "^4.3.1",
    "read-metadata": "^1.0.0",
    "rollup": "^1.1.2",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-typescript2": "^0.19.3",
    "source-map-loader": "^0.2.4",
    "source-map-support": "^0.5.10",
    "style-loader": "^0.23.1",
    "terser-webpack-plugin": "^1.2.1",
    "to-camel-case": "^1.0.0",
    "tslib": "^1.9.3",
    "typescript": "^3.3.3333",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14"
  },
  "peerDependencies": {
    "@gitbook/slate": "*",
    "@gitbook/slate-auto-replace": "*",
    "@gitbook/slate-edit-blockquote": "*",
    "@gitbook/slate-edit-code": "*",
    "@gitbook/slate-edit-list": "*",
    "@gitbook/slate-edit-table": "*",
    "@gitbook/slate-html-serializer": "*",
    "@gitbook/slate-plain-serializer": "*",
    "@gitbook/slate-prism": "*",
    "@gitbook/slate-react": "*",
    "@gitbook/slate-schema-violations": "*"
  },
  "scripts": {
    "benchmark": "cross-env COMPARE=compare node --expose-gc ./tmp/benchmark/index.js",
    "benchmark:save": " yarn benchmark:prepare && node --expose-gc ./tmp/benchmark/index.js",
    "benchmark:prepare": "mkdir -p ./tmp && cross-env BABEL_ENV=benchmark babel benchmark --out-dir tmp/benchmark/",
    "bootstrap": "lerna bootstrap",
    "build": "rollup --config ./support/rollup/config.js",
    "build2": "lerna exec --scope @gitbook/slate-dev-logger -- rollup -c=../../rollup.config.js",
    "watch": "lerna exec --no-sort -- rollup -w -c=../../rollup.config.js",
    "clean": "lerna run clean && rm -rf ./node_modules ./dist ./build",
    "gh-pages": "gh-pages --dist ./build",
    "eslint": "eslint benchmark packages/*/src packages/*/test examples/*/*.js examples/dev/*/*.js",
    "prettier": "prettier '**/*.{js,jsx,md,json,css}'",
    "lint": "yarn run eslint && yarn run prettier --list-different",
    "lint:fix": "yarn run eslint --fix && yarn run prettier --write",
    "open": "open http://localhost:8080",
    "release": "yarn build:production && yarn test:only && yarn lint && lerna publish && yarn gh-pages",
    "server": "webpack-dev-server --config ./support/webpack/config.js",
    "start": "npm-run-all --parallel --print-label watch server",
    "pretest": "yarn run build",
    "test": "yarn run test:only",
    "test:only": "cross-env BABEL_ENV=test jest"
  }
}
