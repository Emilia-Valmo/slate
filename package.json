{
  "private": true,
  "name": "@gitbook/slate-packages",
  "workspaces": [
    "packages/*"
  ],
  "devDependencies": {
    "@gitbook/slate-dev-benchmark": "*",
    "chalk": "^1.1.3",
    "commander": "^2.15.1",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.9",
    "emojis": "^1.0.10",
    "emotion": "^9.2.4",
    "faker": "^3.1.0",
    "figures": "^2.0.0",
    "fs-promise": "^1.0.0",
    "gh-pages": "^0.11.0",
    "html-webpack-plugin": "^3.2.0",
    "html-webpack-template": "^6.2.0",
    "image-extensions": "^1.1.0",
    "immutable": ">=4.0.0-rc.12",
    "is-hotkey": "^0.1.1",
    "is-url": "^1.2.2",
    "jest": "^24.0.0-alpha.9",
    "jsdom": "^11.5.1",
    "lerna": "^2.7.1",
    "lodash": "^4.17.4",
    "mini-css-extract-plugin": "^0.5.0",
    "npm-run-all": "^4.1.2",
    "prismjs": "^1.5.1",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-emotion": "^9.2.4",
    "react-hot-loader": "^4.6.3",
    "react-portal": "^4.1.5",
    "react-router-dom": "^4.3.1",
    "read-metadata": "^1.0.0",
    "rollup": "^1.1.2",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-typescript2": "^0.19.3",
    "source-map-loader": "^0.2.4",
    "source-map-support": "^0.5.10",
    "style-loader": "^0.23.1",
    "terser-webpack-plugin": "^1.2.1",
    "to-camel-case": "^1.0.0",
    "tslib": "^1.9.3",
    "tslint": "^5.13.1",
    "tslint-config-gitbook": "^1.0.0",
    "typescript": "^3.3.3333",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14"
  },
  "resolutions": {
    "prettier": "1.16.4"
  },
  "scripts": {
    "benchmark": "cross-env COMPARE=compare node --expose-gc ./tmp/benchmark/index.js",
    "benchmark:save": " yarn benchmark:prepare && node --expose-gc ./tmp/benchmark/index.js",
    "benchmark:prepare": "mkdir -p ./tmp && cross-env BABEL_ENV=benchmark babel benchmark --out-dir tmp/benchmark/",
    "bootstrap": "lerna bootstrap",
    "build": "rollup --config ./support/rollup/config.js",
    "watch": "rollup --watch --config ./support/rollup/config.js",
    "clean:packages": "lerna exec --no-sort -- rm -rf node_modules dist lib",
    "clean": "yarn run clean:packages",
    "gh-pages": "gh-pages --dist ./build",
    "lint": "tslint ./packages/*/src/**/*.ts",
    "release": "yarn build:production && yarn test:only && yarn lint && lerna publish && yarn gh-pages",
    "server": "webpack-dev-server --config ./support/webpack/config.js",
    "start": "npm-run-all --parallel --print-label watch server",
    "pretest": "yarn run build",
    "test": "yarn run test:only",
    "test:only": "cross-env BABEL_ENV=test jest"
  }
}
